<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Login</title>
</head>
<body>
  <h2>Admin Login</h2>
  <form method="post" action="/login">
    <div>
      <label>Username</label>
      <input name="username" />
    </div>
    <div>
      <label>Password</label>
      <input type="password" name="password" />
    </div>
    <button type="submit">Login</button>
  </form>
</body>
</html>
